AAA,OATA,CATA;AAoBA,MAAA,QAAA,EAAA,QApBA;AAqBA,MAAA,OAAA,EAAA,OArBA;AAsBA,MAAA,WAAA,EAAA;AAtBA,KAAA;AAwBA,GA9BA;AA+BA,EAAA,OA/BA,qBA+BA;AACA,SAAA,OAAA;AACA,GAjCA;AAkCA,EAAA,OAAA,EAAA;AACA,IAAA,OADA,mBACA,OADA,EACA;AACA,UAAA,OAAA,EAAA,KAAA,QAAA,CAAA,IAAA,GAAA,OAAA;AACA,UAAA,UAAA,GAAA;AACA,QAAA,IAAA,EAAA,KAAA,QAAA,CAAA,IADA;AAEA,QAAA,KAAA,EAAA,KAAA,QAAA,CAAA,KAFA;AAGA;AACA,QAAA,UAAA,EAAA;AAJA,OAAA;AAMA,UAAA,KAAA,SAAA,EAAA,UAAA,CAAA,WAAA,GAAA,KAAA,SAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,8CAAA,EAAA;AACA,QAAA,MAAA,EAAA;AADA,OAAA,EAGA,IAHA,CAGA,UAAA,GAAA,EAAA;AACA;AACA,YAAA,GAAA,CAAA,IAAA,IAAA,GAAA,CAAA,IAAA,CAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,cAAA,GAAA,MAAA,CAAA,MAAA,CAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,cAAA,CAAA;AACA;AACA;;AACA,aAAA,IAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,aAAA,QAAA,CAAA,KAAA,GAAA,QAAA,CAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAAA;AACA,OAZA;AAaA,KAvBA;AAwBA,IAAA,gBAxBA,4BAwBA,GAxBA,EAwBA;AACA;AACA,WAAA,OAAA,CAAA,GAAA;AACA,KA3BA;AA4BA,IAAA,mBA5BA,+BA4BA,GA5BA,EA4BA;AACA,WAAA,OAAA,CAAA,GAAA;AACA,KA9BA;AA+BA,IAAA,WA/BA,yBA+BA;AACA,MAAA,OAAA,CAAA,IAAA,CAAA,EAAA;AACA;AAjCA;AAlCA,CAAA","sourcesContent":["\n<template>\n  <div class=\"company travelog\">\n    <div class=\"company-banner\">\n      <HeaderMenu activeIndex=\"6\"></HeaderMenu>\n      <!-- <img src=\"../assets/img/bannerYJ.jpg\" alt /> -->\n      <div class=\"com-search container\">\n        <el-input placeholder=\"如：游记标题\" v-model=\"searchVal\"></el-input>\n        <img class=\"com-search-btn\" @click=\"getList\" src=\"../assets/img/header/search.png\" alt />\n      </div>\n    </div>\n    <div class=\"container\">\n\n      <!-- 搜索内容为空 -->\n      <div class=\"kong\" v-if=\"list.length===0\">\n        <img src=\"../assets/img/kong.svg\" alt=\"\">\n        <p v-if=\"searchVal\">\n          您搜索的\"{{searchVal}}\"暂无内容\n        </p>\n        <p v-if=\"!searchVal\">\n          暂无内容\n        </p>\n      </div>\n      <!-- 过滤选项 start -->\n      <!-- <div class=\"travelog-filter\">\n        <el-row type=\"flex\" justify=\"space-around\">\n          <el-col v-for=\"(item,index) in filter\" :key=\"index\">\n            <div class=\"filter-option\">\n              <span>{{item.option}}</span>\n            </div>\n          </el-col>\n        </el-row>\n      </div> -->\n      <!-- 过滤选项 end -->\n      <!-- 游记 item -->\n      <div class=\"travelog-item\">\n        <el-row :gutter=\"30\">\n          <el-col :span=\"8\" v-for=\"item in list\" :key=\"item.id\">\n            <div class=\"item-detail\">\n              <div class=\"item-img\">\n                <img :src=\"item.coverimg\" />\n                <!-- <div class=\"tag tag-lead\">头条</div>\n                <div class=\"tag tag-cream\">精华</div> -->\n              </div>\n              <div class=\"item-info\">\n                <p v-html=\"item.articlecontent\">\n                </p>\n                <!-- <div class=\"item-active\">\n                  <span class=\"item-read\">\n                    <img :src=\"iconRead\" />\n                    <span>17856</span>\n                  </span>\n                  <span class=\"item-like\">\n                    <img :src=\"iconLik\" />\n                    <span>78</span>\n                  </span>\n                  <span class=\"item-comment\">\n                    <img :src=\"iconComment\" />\n                    <span>30</span>\n                  </span>\n                </div> -->\n              </div>\n            </div>\n          </el-col>\n        </el-row>\n      </div>\n      <!-- 游记 item end -->\n      <!-- 分页 start -->\n      <div class=\"page\" v-if=\"list.length > 0\">\n        <div class=\"block\">\n          <el-pagination\n            background\n            @size-change=\"handleSizeChange\"\n            @current-change=\"handleCurrentChange\"\n            :current-page.sync=\"pageInfo.page\"\n            :page-size=\"pageInfo.limit\"\n            layout=\"prev, pager, next, jumper\"\n            :total=\"pageInfo.total\"\n          ></el-pagination>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport iconRead from \"../assets/img/icon-1.png\";\nimport iconLik from \"../assets/img/icon-2.png\";\nimport iconComment from \"../assets/img/icon-3.png\";\nimport HeaderMenu from \"../components/HeaderMenu.vue\";\n\nlet Base64 = require('js-base64').Base64\nexport default {\n  name: \"Travelog\",\n  components: {\n    HeaderMenu\n  },\n  data() {\n    return {\n      list: [],\n      searchVal: \"\",\n      activeIndex: 1,\n      pageInfo: {\n        page: 1,\n        limit: 15,\n        total: 0\n      },\n      filter: [\n        { option: \"全部\" },\n        { option: \"国内\" },\n        { option: \"亚洲\" },\n        { option: \"欧洲\" },\n        { option: \"北美洲\" },\n        { option: \"大洋洲\" },\n        { option: \"其他州\" },\n        { option: \"和谁出行\" },\n        { option: \"游记玩法\" }\n      ],\n      iconRead: iconRead,\n      iconLik: iconLik,\n      iconComment: iconComment\n    };\n  },\n  mounted() {\n    this.getList();\n  },\n  methods: {\n    getList(pageval) {\n      if (pageval) this.pageInfo.page = pageval;\n      var paramsData = {\n        page: this.pageInfo.page,\n        limit: this.pageInfo.limit,\n        // 文章分类，，，邮轮游记\n        categoryid: 2\n      };\n      if (this.searchVal) paramsData.shipcompany = this.searchVal;\n      this.$http.get(\"/API/article.ashx?command=GetArticleBySearch\", {\n          params: paramsData\n        })\n        .then(function(res) {\n          // 转义内容\n          if(res.body && res.body.list){\n            for(var i=0; i<res.body.list.length; i++){\n              res.body.list[i].articlecontent = Base64.decode(res.body.list[i].articlecontent)\n            }\n          }\n          this.list = res.body.list;\n          this.pageInfo.total = parseInt(res.body.count);\n        });\n    },\n    handleSizeChange(val) {\n      // console.log(`每页 ${limit} 条`);\n      this.getList(val);\n    },\n    handleCurrentChange(val) {\n      this.getList(val);\n    },\n    currentPage() {\n      console.info(11);\n    }\n  }\n};\n</script>\n<style lang=\"scss\">\n.travelog-filter {\n  width: 1180px;\n  height: 60px;\n  box-shadow: 0 0 4px 1px rgba(0, 0, 0, 0.35);\n  margin-top: 60px;\n  padding: 20px;\n  font-size: 16px;\n  font-weight: 700;\n  text-align: center;\n  & .el-col .filter-option {\n    border-right: 1px solid #ccc;\n  }\n  & .el-col:last-child .filter-option {\n    border: none;\n  }\n}\n.travelog-item {\n  width: 1180px;\n  & .item-detail {\n    margin-top: 30px;\n    width: 100%;\n    border: 1px solid #ccc;\n    box-shadow: 0 0 2px 1px rgba(0, 0, 0, 0.1);\n    position: relative;\n    & .item-img {\n      max-width: 100%;\n      img{\n        max-width: 100%;\n      }\n      height: 280px;\n      overflow: hidden;\n      background: #cacaca;\n      & .tag {\n        position: absolute;\n        left: 0;\n        width: 54px;\n        height: 30px;\n        line-height: 30px;\n        color: #fff;\n        font-size: 16px;\n        padding-left: 10px;\n        letter-spacing: 1px;\n        border-radius: 0 30px 30px 0;\n        &.tag-lead {\n          top: 40px;\n          background: orange;\n        }\n        &.tag-cream {\n          top: 85px;\n          background: orangered;\n        }\n      }\n    }\n    & .item-info {\n      padding: 20px;\n      height: 67px;\n      overflow: hidden;\n      & p {\n        font-size: 16px;\n        overflow: hidden;\n        text-overflow: ellipsis;\n        display: -webkit-box;\n        -webkit-line-clamp: 2;\n        -webkit-box-orient: vertical;\n      }\n      & .item-active {\n        margin-top: 10px;\n        font-size: 12px;\n        color: gray;\n        & span {\n          margin-right: 10px;\n          & img {\n            margin-right: 5px;\n          }\n        }\n      }\n    }\n  }\n}\n// 分页\n.page {\n  width: 100%;\n  text-align: center;\n  .block {\n    padding: 30px 0;\n    text-align: center;\n    display: inline-block;\n    box-sizing: border-box;\n    margin: 0 auto;\n  }\n}\n</style>\n"],"sourceRoot":"src/views"}]}